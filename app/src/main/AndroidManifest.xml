<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.supergrocery">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/AppTheme">
        <activity android:name=".other.MainSearchActivity"/>
        <activity android:name=".productmodelactivity.SelectedProductActivity" />
        <activity android:name=".productmodelactivity.DiscountedProductsActivity" />
        <activity android:name=".productmodelactivity.FreeDeliveryActivity" />
        <activity android:name=".payment.PaymentActivity" />
        <activity android:name=".loginregisteractivity.LoginCodeActivity" />
        <activity android:name=".loginregisteractivity.RegisterActivity" />
        <activity android:name=".productmodelactivity.ProductsActivity" />
        <activity android:name=".loginregisteractivity.LoginActivity" />
        <activity android:name=".intro.IntroActivity_1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>